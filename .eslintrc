{
	"env": {
		"es6": true,
		"browser": true,
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"plugins": [
		"jsdoc",
		"@typescript-eslint"
	],
	"settings": {
		"jsdoc": {
			"mode": "typescript"
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"quotes": [
			2,
			"single",
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		],
		"no-eval": [
			2
		],
		"@typescript-eslint/no-this-alias": [
			1,
		],
		"@typescript-eslint/no-explicit-any": 0,
